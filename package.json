{
  "name": "homebridge-assistant-ui",
  "version": "0.0.1-alpha.1",
  "description": "Provides a home appliance operation assistant using a 3D avatar and a log collection function like PDS.",
  "author": "sho <sskmy1024.y@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/sskmy1024y/homebridge-assistant-ui.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e": "NODE_ENV=test jest --forceExit --detectOpenHandles --verbose=true --silent --config ./test/jest-e2e.json",
    "migrate": "nest build && typeorm migration:run"
  },
  "engines": {
    "node": ">=10.17.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-fastify": "^7.4.4",
    "@nestjs/typeorm": "^7.1.4",
    "fastify-multipart": "^3.3.0",
    "fastify-static": "^3.3.0",
    "fs-extra": "^9.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "homebridge-config-ui-x": "^4.32.0"
  }
}
